pipeline {
    agent {
        docker {
            image 'python:3.10-slim'  // Lightweight Python 3.10 image
        }
    }
    stages {
        stage('Install dependencies') {
            steps {
                sh 'pip install requests'
            }
        }
        stage('Run script') {
            steps {
                sh 'python prime.py'
            }
        }
    }
}

