pipeline {
    agent {
    stages {
     stage('Install dependencies') {
            steps {
                // Upgrade pip and install requirements
                sh '''
                    python3 -m pip install --upgrade pip
                    pip3 install -r requirements.txt
                  }      '''
        stage('Run Python Script') {
            steps {
                    sh '''
                        pip install requests
                        python prime.py
                    '''
                }
            }
        }
    }
}





