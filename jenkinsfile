pipeline {
    agent {
        docker {
            image 'python:3.10-slim'
            args '-v $WORKSPACE:$WORKSPACE'  // Mount workspace so script can run
        }
    }
    stages {
        stage('Run IP Info Script') {
            steps {
                // Make sure dependencies installed and run your script
                sh '''
                   pip install requests
                   python prime.py
                '''
            }
        }
    }
}




